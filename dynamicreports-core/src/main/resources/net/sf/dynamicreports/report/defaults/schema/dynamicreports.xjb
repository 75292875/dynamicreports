<?xml version="1.0" encoding="UTF-8"?>
<bindings xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://java.sun.com/xml/ns/jaxb" version="2.0"
          xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd">

    <bindings schemaLocation="dynamicreports.xsd" node="/xsd:schema">
        <schemaBindings>
            <package name="net.sf.dynamicreports.report.defaults.xml"/>
            <nameXmlTransform>
                <typeName prefix="Xml"/>
            </nameXmlTransform>
        </schemaBindings>
    </bindings>
</bindings>
